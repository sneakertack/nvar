{
  "name": "nvar",
  "version": "1.2.0",
  "description": "Reads shell environment variables from a file and assigns them to `process.env` (or anywhere else).",
  "main": "lib/index.js",
  "engines": {
    "node": ">=0.10"
  },
  "scripts": {
    "prepare": "npm run transpile",
    "transpile": "babel lib/nvar.js -o lib/nvar-es5.js",
    "test": "npm run -s test-tap | tap-spec",
    "test-cov": "nyc -s npm run -s test-tap | tap-spec; nyc report",
    "test-tap": "tape tests"
  },
  "keywords": [
    "env",
    "environment",
    "variable",
    "dotenv",
    "envar",
    "config",
    "shell",
    "settings",
    "init",
    "development",
    "preload"
  ],
  "bugs": "https://github.com/sneakertack/nvar/issues",
  "repository": "sneakertack/nvar",
  "author": "Melvin Tiong",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "nyc": "^11.0.3",
    "sinon": "^2.4.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.7.0"
  },
  "babel": {
    "presets": [
      ["env", {
        "targets": {
          "node": "0.10"
        }
      }]
    ]
  },
  "nyc": {
    "exclude": [
      "tests"
    ]
  }
}
