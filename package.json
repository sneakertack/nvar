{
  "name": "nvar",
  "version": "1.0.2",
  "description": "Adds environment variables declared in a `.env` file to your app's `process.env`. Useful during development.",
  "main": "lib/index.js",
  "engines": {"node": ">=6.0"},
  "scripts": {
    "test": "npm run -s test-tap | tap-spec",
    "test-cov": "nyc -s npm run -s test-tap | tap-spec; nyc report",
    "test-tap": "tape tests/**/*.js"
  },
  "keywords": ["env", "environment", "variable", "dotenv", "envar", "config", "shell", "settings", "init", "development", "preload"],
  "bugs": "https://github.com/sneakertack/nvar/issues",
  "repository": "sneakertack/nvar",
  "author": "Melvin Tiong",
  "license": "MIT",
  "devDependencies": {
    "nyc": "^11.0.3",
    "tap-spec": "^4.1.1",
    "tape": "^4.7.0"
  },
  "nyc": {
    "exclude": [
      "tests"
    ]
  }
}
